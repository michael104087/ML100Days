    "import numpy as np \n",
    "import matplotlib.pyplot as plt \n",
    "b=5\n",
    "x_lin = np.linspace(0,10,11)\n",
    "y_hat = x_lin+b\n",
    "\n",
    "def mean_square_error(y,yp):\n",
    "    mse=sum((y-yp)**2)/len(y)\n",
    "    return mse\n",
    "mse=MSE=mean_square_error(x_lin,y_hat)\n",
    "print(MSE)\n",
    "\n",
    "#為了好檢查，將資料數設為11，且方差平方皆為25"
